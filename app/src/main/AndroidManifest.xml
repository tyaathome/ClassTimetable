<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.tyaathome.classtimetable"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.home.ActivityHome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <!-- settings -->
        <activity
            android:name=".view.activity.settings.ActivitySettings"
            android:exported="false"
            android:screenOrientation="portrait"/>

        <!-- add -->
        <activity
            android:name=".view.activity.add.ActivityAddTimetable"
            android:exported="false"
            android:screenOrientation="portrait"/>
    </application>

</manifest>